<!DOCTYPE html>
<html>
  <head>
    <title>FOTO-BOX</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <link href="https://fonts.googleapis.com/css?family=Lobster|Roboto|Satisfy|Source+Sans+Pro|Yellowtail&display=swap" rel="stylesheet">
    <% if logged_in? %>
      <script type="text/javascript">
        window.currentUser = <%= render("api/users/user.json.jbuilder", user: current_user).html_safe %>
      </script>
    <% end %>
    <%= stylesheet_link_tag 'application', media: "all"%>
    
    <%= javascript_include_tag 'application'%>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
